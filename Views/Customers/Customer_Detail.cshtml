@model Book_Rental.Models.Customer
@{
    ViewBag.Title = "Customer Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<p> Membership Type: @Model.MembershipType.Name</p>


@if (Model.BirthDate != null)
{
    <p> Date of Birth: @Model.BirthDate.Value.ToShortDateString()</p>
}